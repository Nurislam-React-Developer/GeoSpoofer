{
	"manifest_version": 2,
	"name": "GeoSpoofer",
	"version": "1.0",
	"description": "Подменяет геолокацию на фиксированные координаты",
	"permissions": ["geolocation", "<all_urls>"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"run_at": "document_start"
		}
	],
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
